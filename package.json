{
  "name": "cloudflare-workers",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "build": "pnpm -r build",
    "build:assets": "pnpm --filter assets.worker build",
    "build:projectrc": "pnpm --filter projectrc.worker build",
    "build:vscode": "pnpm --filter vscode.worker build",
    "dev:all": "pnpm -r dev",
    "dev:assets": "pnpm --filter assets.worker dev",
    "dev:projectrc": "pnpm --filter projectrc.worker dev",
    "dev:vscode": "pnpm --filter vscode.worker dev",
    "lint": "eslint .",
    "typecheck": "pnpm -r tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.8.3",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "typescript": "^5.5.3",
    "wrangler": "^3.63.1"
  }
}
