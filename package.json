{
  "name": "cloudflare-workers",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "pnpm -r build",
    "build:assets": "pnpm --filter assets build",
    "build:vscode": "pnpm --filter vscode build",
    "build:image": "pnpm --filter image build",
    "test": "pnpm vitest --run",
    "test:watch": "pnpm vitest",
    "dev:all": "pnpm -r dev",
    "dev:assets": "pnpm --filter assets dev",
    "dev:vscode": "pnpm --filter vscode dev",
    "dev:image": "pnpm --filter image dev",
    "lint": "pnpm -r lint",
    "lint:assets": "pnpm --filter assets lint",
    "lint:vscode": "pnpm --filter vscode lint",
    "lint:image": "pnpm --filter image lint",
    "typecheck": "pnpm -r typecheck",
    "cf-typegen": "pnpm -r cf-typegen"
  },
  "devDependencies": {
    "@luxass/eslint-config": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "typescript": "catalog:dev",
    "vitest": "catalog:dev",
    "wrangler": "catalog:dev"
  }
}
