{
  "name": "cloudflare-workers",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "build": "pnpm -r build",
    "build:assets": "pnpm --filter assets.worker build",
    "build:vscode": "pnpm --filter vscode.worker build",
    "build:mosaic": "pnpm --filter mosaic.worker build",
    "build:image": "pnpm --filter image.worker build",
    "dev:all": "pnpm -r dev",
    "dev:assets": "pnpm --filter assets.worker dev",
    "dev:vscode": "pnpm --filter vscode.worker dev",
    "dev:mosaic": "pnpm --filter mosaic.worker dev",
    "dev:image": "pnpm --filter image.worker dev",
    "lint": "eslint .",
    "typecheck": "pnpm -r tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "typescript": "catalog:",
    "wrangler": "catalog:"
  }
}
